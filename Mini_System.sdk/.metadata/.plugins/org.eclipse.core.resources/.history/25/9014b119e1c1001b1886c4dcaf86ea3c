#include "include.hpp"

void square_wave(int &volt,int &edge,int &counter)
{
	if(counter >= 0xf)
	{
		if(edge == 1)
			edge = 0;
		else
			edge = 1;
		counter = 0;
	}
	if(edge == 0)
		volt = 0xfff;
	else
		volt = 0;
	counter++;
}
